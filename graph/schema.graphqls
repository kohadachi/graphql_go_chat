# GraphQL schema example
#
# https://gqlgen.com/getting-started/


type Message {
	user: String!
	message: String!
}

type Mutation {
  postMessage(user: String!, message: String!): Message
	createUser(user: String!): String!
}

type Query {
  users: [String!]!
}

type Subscription {
	messagePosted(user: String!): Message!
	userJoined(user: String!): String!
}